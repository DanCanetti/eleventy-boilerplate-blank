<meta property="og:locale" content="en_GB" />
<meta property="og:site_name" content="{{ siteInfo.siteTitle }}" />
<meta property="og:title" content="{{ siteInfo.siteTitle }} • {{ pageTitle }}" />
<meta property="og:description" content="{{ pageDesc }}" />
<meta property="og:url" content="{{ siteInfo.url }}/" />
<meta property="og:image" content="{{ siteInfo.url }}{{ pageFeatureImage }}" />
<meta property="twitter:card" content="summary_large_image">
<meta property="twitter:title" content="{{ siteInfo.siteTitle }} • {{ pageTitle }}">
<meta property="twitter:description" content="{{ pageDesc }}">
<meta property="twitter:image" content="{{ siteInfo.url }}{{ pageFeatureImage }}">
<meta property="twitter:url" content="{{ siteInfo.url }}/">
<meta property="twitter:site" content="{{ siteInfo.twitterHandle }}">
<meta property="twitter:creator" content="{{ siteInfo.twitterHandle }}">